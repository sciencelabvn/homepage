{
  "private": true,
  "scripts": {
    "start": "wmr",
    "build": "wmr build --prerender",
    "serve": "wmr serve",
    "ci:lint": "yarn eslint . && yarn prettier --log-level warn --check \"**/*.{tsx,jsx,mjs,js,html,css,json}\"",
    "lint": "yarn eslint . --fix && yarn prettier --log-level warn --write \"**/*.{tsx,jsx,mjs,js,html,css,json}\" && yarn eslint ."
  },
  "eslintConfig": {
    "extends": "preact"
  },
  "dependencies": {
    "eslint": "^8.48.0",
    "eslint-config-preact": "^1.3.0",
    "preact": "^10.17.1",
    "preact-iso": "^2.3.1"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^6.4.1",
    "@typescript-eslint/parser": "^6.4.1",
    "eslint-config-prettier": "^9.0.0",
    "prettier": "^3.0.2",
    "typescript": "^5.2.2",
    "wmr": "^3.8.0"
  }
}
